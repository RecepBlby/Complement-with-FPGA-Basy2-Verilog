NET "switch<2>" LOC = "K3";  # Bank = 3, Signal name = SW2
NET "switch<1>" LOC = "L3";  # Bank = 3, Signal name = SW1
NET "switch<0>" LOC = "P11";  # Bank = 2, Signal name = SW0
